# Public (client) env vars for Vite
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# --- PORT REGISTRY (DO NOT CHANGE WITHOUT UPDATING BUILD_CONTRACT) ---
VITE_DEV_PORT=5173
VITE_PREVIEW_PORT=4173

API_PORT=8787
COINBASE_WEBHOOK_PORT=8788

# --- DEV AUTH BYPASS (NEVER ENABLE IN PRODUCTION) ---
# Set to '1' to bypass Google SSO during local development
# Backend will accept x-dev-user-id header instead of JWT
# Frontend will skip auth guards and use dev user ID
VITE_DEV_AUTH_BYPASS=0
VITE_DEV_USER_ID=dev_user

DEV_AUTH_BYPASS=0
DEV_USER_ID=dev_user
